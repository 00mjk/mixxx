	/* ##################################################################################################################### */
	/* GLOBAL STYLES: */	
	/* ##################################################################################################################### */

	/* Main Window (Doesnt work by now) */
	QWindow { border: 1px solid #3c3c3c; background-color: #000000; background: #000000; color: #d5d5d5; }
	
	/* Main Buttons: Backs and theirs texts */
	QPushButton { font: normal 16px/18px Arial; background: #000000; color: #d5d5d5; margin-right:10px; padding: 4px; border: 1px solid #3c3c3c;}
	QPushButton::pressed { background: #404040; color: #d5d5d5; padding: 4px; border: 1px solid #3c3c3c;}
	QPushButton::checked { background: #404040; color: #d5d5d5; padding: 4px; border: 1px solid #3c3c3c;}
	
	/* Tips */
	QToolTip { color: #d5d5d5; border: 1px solid #808080; background-color: transparent; }	

	
	/* ##################################################################################################################### */
	/* STYLES FOR THE LIBRARY: */	
	/* ##################################################################################################################### */	

	/* QT STYLE SHEETS EXAMPLES: http://qt-project.org/doc/qt-5/stylesheet-examples.html */
				
	QGroupBox { border: 0px solid #ffffff; margin: 0px 0px 0px 0px; }				
    QHeaderView { font: "Arial"; font-family: "Arial"; }
			
	/* File Browser SELECTED BAR: */
	QTableView, QTextBrowser, QTreeView { selection-color: #ffffff; selection-background-color: #1f1f1f; color: #ffffff; }	
				
	/* File Browser HOVER BAR: */
	QTableView::item:hover, QTextBrowser::item:hover, QTreeView::item:hover { color: #ffffff; }				

	/* File Browser ROWS: */
	QTableView, QTextBrowser, QTreeView { border: 1px solid #656565; font: "Arial"; font-family: "Arial"; background: transparent; color: #707070; alternate-background-color: #0c0c0c; }
	/* we use "background: transparent" as workaround, else "alternate-background-color" wont work, QT 4.7.x bug? */				
				
	/* checkbox in library "Played" column */
	QTableView::indicator { width: 12px; height: 12px; background: transparent; border: 0; }
	QTableView::indicator:unchecked { background: url(skin:/gfx_library/checkbox.svg); no-repeat; }
	QTableView::indicator:checked { background: url(skin:/gfx_library/checkbox_checked.svg); no-repeat; }	
	
	/* TREE NAVIGATION - BRANCHES IMAGES: */
	QTreeView { show-decoration-selected: 0; } /* Suppresses that selected sidebar items branch indicator shows wrong color when out of focus; lp:880588 */
	QTreeView::branch:has-siblings:!adjoins-item { border-image: url(skin:/gfx_library/branch_line.svg) 0; }
	QTreeView::branch:has-siblings:adjoins-item { border-image: url(skin:/gfx_library/branch_more.svg) 0; }
	QTreeView::branch:!has-children:!has-siblings:adjoins-item { border-image: url(skin:/gfx_library/branch_end.svg) 0; }
	QTreeView::branch:has-children:!has-siblings:closed, QTreeView::branch:closed:has-children:has-siblings {
	border-image: none;	image: url(skin:/gfx_library/branch_closed.svg); }
	QTreeView::branch:open:has-children:!has-siblings, QTreeView::branch:open:has-children:has-siblings {
	border-image: none;	image: url(skin:/gfx_library/branch_open.svg); }
			
	/* Button in library "Preview" column */
	QPushButton#LibraryPreviewButton { font: 10px/12px "Arial"; font-family: "Arial"; margin: 1px; padding: 1px; }
	QPushButton#LibraryPreviewButton { width: 16px; height: 12px; background: transparent; border: 0; }
	QPushButton#LibraryPreviewButton:!checked { image: url(skin:/gfx_library/preview_play.svg); no-repeat; }
	QPushButton#LibraryPreviewButton:checked { image: url(skin:/gfx_library/preview_pause.svg); no-repeat; }
								
	/* QPushButton#RatingButton { width: 16px; height: 12px; background: transparent; border: 0; color: #ffffff; } */

	QSpinBox:editable { font: 10px/12px "Arial"; font-family: "Arial"; background: transparent; color: #e3e3e3; }
	QSpinBox { min-height: 20px; max-height: 20px; min-width: 40px; max-width: 40px;}

	/* library search box bar */
	WSearchLineEdit { padding: 1px; border: 1px solid #656565; font: 13px/15px "Arial"; font-family: "Arial"; background: #000000; color: #e3e3e3; }
	WSearchLineEdit:focus { padding: 2px; border: 1px solid #ffffff; font: normal 10px/12px "Arial"; font-family: "Arial"; background: #000000; color: #D6D6D6;}
		
	/* QSplitter: Between Tree and Library ( QSplitter::handle {...} ) */
	#SplitterVertical::handle { image: url(skin:/gfx_library/handle_vertical.svg); background: none; }
	#SplitterVertical::handle:pressed { image: url(skin:/gfx_library/handle_vertical_pressed.svg); background: none; }
	#SplitterVertical::handle:horizontal { width: 4px; }
	#SplitterVertical::handle:vertical { height: 4px; }

	/* QSplitter: Between Library and CoverArt ( QSplitter::handle {...} )  */
	#SplitterHorizontal::handle { image: url(skin:/gfx_library/handle_horizontal.svg); background: none; }
	#SplitterHorizontal::handle:pressed { image: url(skin:/gfx_library/handle_horizontal_pressed.svg); background: none; }
	#SplitterHorizontal::handle:horizontal { width: 4px; }
	#SplitterHorizontal::handle:vertical { height: 4px; }

	QPushButton { font: 10px/12px "Arial"; font-family: "Arial"; margin: 1px; padding: 10px; }
				
	/* Extra declaration for QRadionButton otherwise it shows up with wrong colors in Linux with Gnome */
	QLabel, QRadioButton { font: 10px/12px "Arial"; font-family: "Arial"; background: transparent; color: #e3e3e3; }

	/* Spacing between treeview and searchbar */
	QTreeView { margin: 6px 0px 0px 0px; }
							
	/* QTreeView: Customizing left tree items navigation: */
	QTreeView::item { background: none; color: #8e8e8e; }
	QTreeView::item:selected { background: #141414; color: #8e8e8e; }
	QTreeView::item:selected:active { background: #141414; color: #8e8e8e; }
	QTreeView::item:hover { background: #141414; color: #8e8e8e; }			
				
	/* File Browser HEADERS: */
	QHeaderView { font: 10px/12px "Arial"; font-family: "Arial"; background: #000000; border-bottom: 1px solid #404040; }
	QHeaderView::section { padding: 2px; background: #5d5d5d; border-top: none; border-bottom: 1px solid #000000; border-left: 1px solid #000000; border-right: none; }
	QHeaderView::section:selected { padding: 2px; background: #E4E4E4; border-top: none; border-bottom: 1px solid #000000; border-left: 1px solid #000000; border-right: none; }
	QHeaderView::up-arrow { image: url(skin:/gfx_library/arrow_up.svg)}
	QHeaderView::down-arrow {image: url(skin:/gfx_library/arrow_down.svg)}		
			
	/* File Browser SCROLL BARS: */
	QAbstractScrollArea::corner { background: #000000; border: none ;}
				
	QScrollBar:horizontal { border-top: 1px solid #aeaeae; background: #101010; height: 14px; margin: 0px 43px 0px 0px; }
	QScrollBar::handle:horizontal { background: #323232; min-width: 20px; } /* horizontal drag bar */
	QScrollBar::handle:horizontal:pressed { background: #323232; min-width: 20px; } /* horizontal drag bar */
	QScrollBar::add-line:horizontal { background: #808080; width: 20px; subcontrol-position: right; subcontrol-origin: margin; border-left: 1px solid #101010; border-right: 1px solid #101010; border-top: 1px solid #aeaeae; }
	QScrollBar::add-line:horizontal:pressed { background: #aeaeae; width: 20px; subcontrol-position: right; subcontrol-origin: margin; border-left: 1px solid #101010; border-right: 1px solid #101010; }
	QScrollBar::sub-line:horizontal { background: #808080; width: 20px; subcontrol-position: top right; subcontrol-origin: margin; position: absolute; right: 21px; border-left: 1px solid #101010; border-right: 1px solid #101010; border-top: 1px solid #aeaeae; }
	QScrollBar::sub-line:horizontal:pressed { background: #aeaeae; width: 20px; subcontrol-position: top right; subcontrol-origin: margin; position: absolute; right: 21px; border-left: 1px solid #101010; border-right: 1px solid #101010; }
	QScrollBar:left-arrow:horizontal, QScrollBar::right-arrow:horizontal { width: 12px; height: 12px; background: none; }
	QScrollBar::add-page:horizontal, QScrollBar::sub-page:horizontal { background: none; }
	QScrollBar::left-arrow:horizontal { image: url(skin:/gfx_library/scroll_left.svg)} QScrollBar::right-arrow:horizontal { image: url(skin:/gfx_library/scroll_right.svg)}

	QScrollBar:vertical { border-left: 1px solid #aeaeae; background: #101010; width: 14px; margin: 0px 0px 43px 0px; }
	QScrollBar::handle:vertical { background: #323232; min-height: 20px; } /* vertical drag bar */
	QScrollBar::handle:vertical:pressed { background: #323232; min-height: 20px; } /* vertical drag bar */
	QScrollBar::add-line:vertical { background: #808080; height: 20px; subcontrol-position: bottom; subcontrol-origin: margin; border-left: 1px solid #aeaeae; border-top: 1px solid #101010; border-bottom: 1px solid #101010;}
	QScrollBar::add-line:vertical:pressed { background: #aeaeae; height: 20px; subcontrol-position: bottom; subcontrol-origin: margin; border-top: 1px solid #101010; border-bottom: 1px solid #101010;}
	QScrollBar::sub-line:vertical { background: #808080; height: 20px; subcontrol-position: top bottom; subcontrol-origin: margin;position: absolute; bottom: 21px; border-left: 1px solid #aeaeae; border-top: 1px solid #101010; border-bottom: 1px solid #101010;}
	QScrollBar::sub-line:vertical:pressed { background: #aeaeae; height: 20px; subcontrol-position: top bottom; subcontrol-origin: margin; position: absolute; bottom: 21px; border-top: 1px solid #101010; border-bottom: 1px solid #101010; }
	QScrollBar::up-arrow:vertical, QScrollBar::down-arrow:vertical { width: 12px; height: 12px; background: none; }
	QScrollBar::add-page:vertical, QScrollBar::sub-page:vertical { background: none; } QScrollBar::add-page:vertical:pressed, QScrollBar::sub-page:vertical:pressed {background: none; }
	QScrollBar::up-arrow:vertical { image: url(skin:/gfx_library/scroll_up.svg)}
	QScrollBar::down-arrow:vertical { image: url(skin:/gfx_library/scroll_down.svg)}
				
	/* QComboBox: Not used */
	QComboBox { border: 1px solid #424242; color: #ffffff; background: transparent; margin: 1px; padding: 1px; }
	QComboBox::up-arrow { image: url(skin:/gfx_library/arrow_up.svg); }
	QComboBox::up-arrow:on { image: url(skin:/gfx_library/arrow_up.svg) }
	QComboBox::down-arrow { image: url(skin:/gfx_library/arrow_down.svg); }
	QComboBox::down-arrow:on { image: url(skin:/gfx_library/arrow_down.svg) }
				
	/* QSpinBox: Used at "Auto DJ - transition duration" */
	QSpinBox { border: 1px solid #424242; color: #ffffff; background: transparent; margin: 1px; padding: 7px; }
	QSpinBox::up-arrow { image: url(skin:/gfx_library/arrow_up.svg); }
	QSpinBox::up-arrow:on { image: url(skin:/gfx_library/arrow_up.svg) }
	QSpinBox::down-arrow { image: url(skin:/gfx_library/arrow_down.svg); }
	QSpinBox::down-arrow:on { image: url(skin:/gfx_library/arrow_down.svg) }
				
	/* QLineEdit: Input texts fields */
	QLineEdit { border: 1px solid #424242; color: #d5d5d5; background: #000000; margin: 1px; padding: 1px; }			
				
	/* QTextEdit: Input texts fields */
	QTextEdit { border: 1px solid #424242; color: #d5d5d5; background: #000000; margin: 1px; padding: 1px; }
	
	
	
	
	
	
	