			<Connection>
			<ConfigKey>[Auxiliary1],PeakIndicator</ConfigKey>
			</Connection>
			</StatusLight>
			
			<!-- Volume: -->			
			<KnobComposed>
			<Pos>107,3</Pos>
			<Size>40f,40f</Size>
			<BackPath>skin:graphics/background_knob1.svg</BackPath>
			<Knob>skin:graphics/pointer_knob1.svg</Knob>
			<MinAngle>-130</MinAngle>
			<MaxAngle>130</MaxAngle>
			<Connection>
			<ConfigKey>[Auxiliary1],pregain</ConfigKey>
			</Connection>
			</KnobComposed>	
			
			<!-- Mute: -->
			<PushButton>
			<Pos>12,6</Pos>
			<NumberStates>2</NumberStates>
			<State>
			<Number>0</Number>
			<Pressed>skin:graphics/btn_mute_1_pressed.svg</Pressed>
			<Unpressed>skin:graphics/btn_mute_1.svg</Unpressed>
			</State>
			<State>
			<Number>1</Number>
			<Pressed>skin:graphics/btn_mute_1_pressed.svg</Pressed>
			<Unpressed>skin:graphics/btn_mute_1_pressed.svg</Unpressed>
			</State>					
			<Connection>
			<ConfigKey>[Auxiliary1],mute</ConfigKey>
			<ButtonState>LeftButton</ButtonState>
			</Connection>
			</PushButton>
	
