<?xml version="1.0" encoding="utf-8"?>
<MixxxMIDIPreset version="1.6.5">
    <info>
        <name>Crash test</name>
        <author>Sean M. Pappalardo</author>
        <description>This is mapping used to test MIDI scripting crashes. Requires scripting, if you haven't figured that out. ;) </description>
    </info>
    <controller id="MidiKeys" port="Port">
        <scriptfiles>
            <file filename="crashTest.js" functionprefix="crashTest"/>
        </scriptfiles>
        
        <controls>
            <control>
                <group>[Master]</group>
                <key>crashTest.swapSignals</key>
                <miditype>Key</miditype>
                <midino>0x2A</midino>
                <midichan>1</midichan>
                <options>
                    <Script-Binding/>
                </options>
            </control>
            <control>
                <group>[Master]</group>
                <key>crashTest.rewind</key>
                <miditype>Key</miditype>
                <midino>0x30</midino>
                <midichan>1</midichan>
                <options>
                    <Script-Binding/>
                </options>
            </control>
            <control>
                <group>[Master]</group>
                <key>crashTest.ffwd</key>
                <miditype>Key</miditype>
                <midino>0x32</midino>
                <midichan>1</midichan>
                <options>
                    <Script-Binding/>
                </options>
            </control>

        </controls>
        
